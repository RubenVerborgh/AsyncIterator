{
  "name": "asynciterator",
  "version": "3.0.0-alpha.0",
  "description": "An asynchronous iterator library for advanced object pipelines.",
  "author": "Ruben Verborgh <ruben@verborgh.org>",
  "type": "module",
  "main": "./asynciterator.cjs",
  "exports": {
    "import": "./asynciterator.js",
    "require": "./asynciterator.cjs"
  },
  "module": "./asynciterator.js",
  "sideEffects": false,
  "files": [
    "asynciterator.js",
    "asynciterator.cjs"
  ],
  "scripts": {
    "build": "babel asynciterator.js > asynciterator.cjs",
    "prepare": "npm run build",
    "test": "mocha",
    "cover": "nyc -- mocha -R dot",
    "lint": "eslint *.js test",
    "docs": "jsdoc -c jsdoc.json README.md *.js"
  },
  "license": "MIT",
  "repository": "RubenVerborgh/AsyncIterator",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/register": "^7.10.1",
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "jaguarjs-jsdoc": "^1.1.0",
    "jsdoc": "^3.5.5",
    "mocha": "^8.0.0",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0"
  },
  "pre-commit": [
    "lint",
    "cover"
  ],
  "dependencies": {
    "queue-microtask": "^1.1.2"
  }
}
