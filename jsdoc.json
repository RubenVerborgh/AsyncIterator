{
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "node_modules/jaguarjs-jsdoc"
  },
  "plugins": [ "plugins/markdown" ],
  "templates": {
    "default": {
      "outputSourceFiles": false
    }
  },
  "source": {
    "include": [
      "README.md",
      "asynciterator.mjs"
    ],
    "includePattern": ".+\\.mjs"
  }
}
